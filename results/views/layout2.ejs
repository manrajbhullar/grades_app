<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://use.fontawesome.com/5311b629e9.js"></script>
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<title>Results</title>
<h1 class="m-4">Results</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Course</th>
      <th scope="col">Highest Grade</th>
      <th scope="col">Average Grade</th>
      <th scope="col">Lowest Grade</th>
      <th scope="col">Timestamp</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 1; i <= update_length; i++) {%>
    <% console.log(body[body_length-i])%>
    <tr>
      <th scope="row"><%=body[body_length-i].name%></th>
      <td><%=body[body_length-i].max%></td>
      <td><%=body[body_length-i].avg%></td>
      <td><%=body[body_length-i].min%></td>
      <td><%=body[body_length-i].timestamp%></td>
    </tr>
    <%};%>
  </tbody>
</table>
